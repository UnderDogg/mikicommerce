  "authors": [
    {
      "name": "Mikiyas Amdu",
      "email": "mikias.amdu@gmail.com",
      "homepage": "https://github.com/mikimaine/ecommerce",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "laravel/framework": "5.2.*",
    "laravelcollective/html": "^5.2",
    "laravel/socialite": "^2.0",
    "stripe/stripe-php": "^3.4.0",
    "hieu-le/active": "^2.0",
    "doctrine/dbal": "~2.5",
    "laracasts/utilities": "^2.1",
    "arcanedev/log-viewer": "^3.9.0",
    "creativeorange/gravatar": "^1.0",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "symfony/css-selector": "^2.1",
    "cviebrock/eloquent-sluggable": "^3.1",

    "spatie/laravel-tail": "^1.1",
    "artesaos/seotools": "^0.8.2",
    "orangehill/iseed": "^2.1",
    "yajra/laravel-datatables-oracle": "~6.0",
    "filp/whoops": "^2.0",
    "sofa/eloquence": " ~5.2@dev",
    "spatie/activitylog": "^2.4",
    "spatie/laravel-url-signer": "^1.1",
    "intervention/image": "^2.3",
    "intervention/imagecache": "^2.3",
    "spatie/laravel-backup": "^3.2",
    "gloudemans/shoppingcart": "^1.3",
    "spatie/laravel-responsecache": "^1.1",
    "barryvdh/laravel-ide-helper": "^2.1",
    "barryvdh/laravel-snappy": "^0.3.1",
    "pagerfanta/pagerfanta": "^1.0",
    "caffeinated/themes": "^2.0",
    "codesleeve/laravel-stapler": "^1.0",
    "lukepolo/laracart": "^1.1",
    "spiritix/lada-cache": "^1.2",
    "league/uri": "^4.2",
    "michelf/php-markdown": "^1.6",
    "maatwebsite/laravel-sidebar": "^2.1"
  },
  "require-dev": {
    "symfony/dom-crawler": "~3.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "phpspec/phpspec": "~2.1",
    "barryvdh/laravel-debugbar": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Innovate\\": "Innovate/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
    ]
  },
  "scripts": {
    "post-install-cmd": [
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": {
    "0": {
      "type": "composer",
      "url": "https://packagist.org"
    },
    "repo-name": {
      "type": "git",
      "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
    }
  }
}
